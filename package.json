    "setDonationThresholds": "npx hardhat run scripts/setDonationThresholds.ts",
    "setChainlinkCallbackGasLimit": "npx hardhat run scripts/setChainlinkCallbackGasLimit.ts",
    "setPaintSwapMarketplaceWhitelist": "npx hardhat run scripts/setPaintSwapMarketplaceWhitelist.ts",
    "setUpgradeCost": "npx hardhat run scripts/setUpgradeCost.ts",
