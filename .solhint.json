{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "func-visibility": ["error", { "ignoreConstructors": true }],
    "code-complexity": ["error", 7],
    "compiler-version": ["error", "<9.0.0"]
  }
}
